{
  "name": "stefanwimmer128.eu",
  "description": "Source code for stefanwimmer128.eu",
  "scripts": {
    "build": "yarn build-server && yarn build-client",
    "build-client": "NODE_ENV=\"${NODE_ENV:-development}\" babel-node --presets=@babel/preset-env \"$(which webpack-cli)\" --config webpack/client.js",
    "build-server": "NODE_ENV=\"${NODE_ENV:-development}\" babel-node --presets=@babel/preset-env \"$(which webpack-cli)\" --config webpack/server.js",
    "deploy": "firebase deploy",
    "dev-server": "NODE_ENV=development babel-node --presets=@babel/preset-env \"$(which webpack-dev-server)\" --config webpack/client.js",
    "serve": "firebase serve"
  },
  "repository": {
    "type": "git",
